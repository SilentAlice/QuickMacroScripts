<script type='text/javascript'>(function() {
	var Slide = function(ele, leftClick, rightClick) {
		this.index = 0;
		var li = ele.getElementsByTagName('li'),
			that = this;
		this.length = li.length;
		this.lefts = [];
		this.iPadX = 0;
		this.iPadLastX = 0;
		this.iPadStatus = 'ok';
		this.dbClickPOP = false;
		this.leftClick = leftClick;
		this.rightClick = rightClick;
		for (var i = 0; i < this.length; i++) {
			this.lefts.push(-li[i].offsetLeft);
		}
		this.ele = ele;
		this.objId = this.ele.id;
		if (typeof(window.ontouchstart) === 'undefined') {
			if (this.length > 1) {
				this.rightClick.style.display = 'block';
			}
			this.ele.className = 'usePic clearfix tranS';
			this.leftClick.addEventListener('click', function(e) {
				that.Pre(leftClick, rightClick);
			}, false);
			this.rightClick.addEventListener('click', function(e) {
				that.Next(leftClick, rightClick);
			}, false);
		} else {
			this.ele.addEventListener('touchstart', function(e) {
				that._touchstart(e);
			});
			this.ele.addEventListener('touchmove', function(e) {
				that._touchmove(e);
			});
			this.ele.addEventListener('touchend', function(e) {
				that._touchend(e);
			});
		}
		ele = li = null;
	};
	Slide.prototype.Pre = function() {
		if (this.index <= 0) {
			return;
		};
		if (this.index <= 1) {
			this.leftClick.style.display = 'none';
		}
		this.rightClick.style.display = 'block';
		this.index -= 1;
		this.ele.style.marginLeft = this.lefts[this.index] + 'px';
	};
	Slide.prototype.Next = function() {
		if (this.index >= this.length - 1) {
			return;
		};
		if (this.index >= this.length - 2) {
			this.rightClick.style.display = 'none';
		}
		this.leftClick.style.display = 'block';
		this.index += 1;
		this.ele.style.marginLeft = this.lefts[this.index] + 'px';
	};
	Slide.prototype._touchstart = function(e) {
		this.iPadX = e.touches[0].pageX;
		this.iPadScrollX = window.pageXOffset;
		this.iPadScrollY = window.pageYOffset;
		this.ele.className = 'usePic clearfix';
	};
	Slide.prototype._touchmove = function(e) {
		if (e.touches.length > 1) {
			this.iPadStatus = 'ok';
			return;
		};
		this.iPadLastX = e.touches[0].pageX;
		this.cX = this.iPadX - this.iPadLastX;
		if (this.iPadStatus == 'ok') {
			if (this.iPadScrollY == window.pageYOffset && this.iPadScrollX == window.pageXOffset && Math.abs(this.cX) > 5) {
				this.iPadStatus = 'touch';
			} else {
				return;
			};
		};
		this._state = 'touch';
		var offLeft = Math.abs(this.lefts[this.index]),
			lastOffLeft = Math.abs(this.lefts[this.lefts.length - 1]),
			scrollNum = this.cX + offLeft;
		if (scrollNum < 0) {
			scrollNum = 0;
		} else if (scrollNum >= lastOffLeft) {
			scrollNum = offLeft;
		}
		this.ele.style.marginLeft = -scrollNum + 'px';
		e.preventDefault();
	};
	Slide.prototype._touchend = function(e) {
		if (this.iPadStatus === 'touch') {
			this.iPadStatus = 'ok';
			this.ele.className = 'usePic clearfix tranS';
			if (this.cX > 0) {
				if (this.index >= this.length - 1) {
					return;
				};
				this.index += 1;
				this.ele.style.marginLeft = this.lefts[this.index] + 'px';
			} else {
				if (this.index <= 0) {
					return;
				};
				this.index -= 1;
				this.ele.style.marginLeft = this.lefts[this.index] + 'px';
			}
		}};
	var itemPic = document.querySelectorAll('#usePic .usePic'),
		clickLeftBtn = document.querySelectorAll('#usePic .clickLeft'),
		clickRightBtn = document.querySelectorAll('#usePic  .clickRight'),
		length = itemPic.length;
	window.slides = [];
	setTimeout(function() {
		for (var i = 0; i < length; i++) {
			window.slides.push(new Slide(itemPic[i], clickLeftBtn[i], clickRightBtn[i]));
		}
	}, 2500);})();</script>
